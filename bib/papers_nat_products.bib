@article{kuckelhaus_phylloseptin-1_2020,
	title = {Phylloseptin-1 is Leishmanicidal for Amastigotes of Leishmania amazonensis Inside Infected Macrophages},
	volume = {17},
	rights = {https://creativecommons.org/licenses/by/4.0/},
